<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.game.RoomListController" >
    <top>
        <HBox fillHeight="true" >
            <Button fx:id="createButton" onAction="#add" text="Utwórz pokój" prefWidth="200" HBox.hgrow="ALWAYS" maxWidth="Infinity" />
            <Button fx:id="joinButton" onAction="#join" text="Dołącz" prefWidth="200" HBox.hgrow="ALWAYS" maxWidth="Infinity" />
        </HBox>
    </top>
    <center>
        <ListView fx:id="list" >
            <items>
                <FXCollections fx:factory="observableArrayList">
                </FXCollections>
            </items>
        </ListView>
    </center>
</BorderPane>
